{
  "name": "appian-browser-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "popper.js": "^1.14.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.5.3",
    "react-linkify": "^0.2.2",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "precommit": "pretty-quick staged",
    "start": "react-app-rewired start",
    "start:firefox": "web-ext run",
