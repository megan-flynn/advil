{
  "manifest_version": 2,
  "name": "test123",
  "description": "test 123",
  "version": "1.0.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "browserextension@appian.com",
      "strict_min_version": "57.0"
    }
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    }
  },
  "background": {
    "scripts": ["static/js/eventPage.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["static/js/gmail.js"],
      "css": ["gmail.css"]
    }
  ],
  "web_accessible_resources": ["icon16.png"],
  "permissions": ["history", "storage", "alarms", "activeTab", "<all_urls>"],
  "content_security_policy":
    "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
